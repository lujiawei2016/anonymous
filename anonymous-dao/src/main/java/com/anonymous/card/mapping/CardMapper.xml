<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.anonymous.card.dao.CardDao">

	<!-- 保存卡片 -->
	<insert id="saveCard" parameterType="hashmap">
		INSERT an_card (
		  cardId,
		  cardContent,
		  cardImg,
		  createTime,
		  updateTime,
		  anonymId
		) 
		VALUES (
		  #{cardId},
		  #{cardContent},
		  #{cardImg},
		  #{createTime},
		  #{updateTime},
		  #{anonymId}
		)
	</insert>
	
	<!-- 查出最新卡片 -->
	<select id="searchNewCard" resultType="com.anonymous.card.pojo.Card">
	</select>

</mapper>